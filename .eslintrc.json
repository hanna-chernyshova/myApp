 {
    "extends": "eslint:recommended",
    "plugins": [
        "standard",
        "promise",
        "react"
    ],

"parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "script",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "semi": 2,
        "no-unused-vars":"off"
    }
  
}